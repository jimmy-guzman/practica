{
  "name": "practica",
  "version": "0.0.0",
  "description": "solved coding problems",
  "keywords": [],
  "author": "jimmy-guzman @jimmy-guzman",
  "license": "MIT",
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "tsc": "tsc --noEmit",
    "validate": "run-p lint format tsc test:coverage",
    "prepare": "is-ci || husky install"
  },
  "gitzy": {
    "useCommitlintConfig": true
  },
  "lint-staged": {
    "*.{md,json,yml,ts}": [
      "prettier -w"
    ],
    "*.ts": [
      "eslint --cache"
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.0.2",
    "@types/node": "16.11.38",
    "c8": "7.11.3",
    "commitlint": "17.0.2",
    "eslint": "8.17.0",
    "eslint-config-jimmy-guzman": "5.0.0",
    "gitzy": "3.2.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "typescript": "4.9.5",
    "vitest": "0.13.1"
  },
  "volta": {
    "node": "16.15.1"
  }
}
